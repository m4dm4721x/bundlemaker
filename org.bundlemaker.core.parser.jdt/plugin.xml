<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="jdtsourceparserhook" name="jdtsourceparserhook" schema="schema/jdtsourceparserhook.exsd"/>
   
   <extension
         point="org.bundlemaker.core.parserfactory">
      <parserfactory
            class="org.bundlemaker.core.parser.jdt.internal.JdtParserFactory">
      </parserfactory>
   </extension>
   
   <extension
       point="org.eclipse.jdt.ui.javaElementFilters">
    <filter
          class="org.bundlemaker.core.parser.jdt.internal.JavaElementFilter"
          description="Filters any bundlemaker JDT project."
          enabled="true"
          id="filter-id"
          name="FilterName">
    </filter>
 </extension>

</plugin>
