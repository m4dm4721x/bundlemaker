<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension-point id="jdtsourceparserhook" name="jdtsourceparserhook" schema="schema/jdtsourceparserhook.exsd"/>
   
   <extension
         point="org.bundlemaker.core.parserfactory">
      <parserfactory
            class="org.bundlemaker.core.jdt.internal.parser.JdtParserFactory">
      </parserfactory>
   </extension>

   <extension
         id="org.bundlemaker.core.jdt.content.JdtProjectContentProvider"
         name="JdtProjectContentProvider"
         point="org.bundlemaker.core.projectcontentprovider">
      <projectContentProvider
            class="org.bundlemaker.core.jdt.content.JdtProjectContentProvider">
         <jaxbObjectFactory
               class="org.bundlemaker.core.jdt.content.xml.ObjectFactory">
         </jaxbObjectFactory>
      </projectContentProvider>
   </extension>
</plugin>
